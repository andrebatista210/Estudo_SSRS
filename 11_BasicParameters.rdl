  <DataSets>
    <DataSet Name="DS_Filmes">
      <Query>
        <DataSourceName>DS_Films</DataSourceName>
        <CommandText>SELECT   FilmName, FilmReleaseDate, FilmRunTimeMinutes, FilmOscarWins
FROM         tblFilm</CommandText>
      </Query>
      <Fields>
        <Field Name="FilmName">
          <DataField>FilmName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FilmReleaseDate">
          <DataField>FilmReleaseDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="FilmRunTimeMinutes">
          <DataField>FilmRunTimeMinutes</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="FilmOscarWins">
          <DataField>FilmOscarWins</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!FilmRunTimeMinutes.Value</FilterExpression>
          <Operator>LessThanOrEqual</Operator>
          <FilterValues>
            <FilterValue>=Parameters!MinRunTime.Value</FilterValue>
          </FilterValues>
        </Filter>
        <Filter>
          <FilterExpression>=Fields!FilmReleaseDate.Value</FilterExpression>
          <Operator>Between</Operator>
          <FilterValues>
            <FilterValue>=Parameters!DT_INICIO.Value</FilterValue>
            <FilterValue>=Parameters!DT_FIM.Value</FilterValue>
          </FilterValues>
        </Filter>
        <Filter>
          <FilterExpression>=Fields!FilmName.Value</FilterExpression>
          <Operator>Like</Operator>
          <FilterValues>
            <FilterValue>="*" &amp; Parameters!NomeFilme.Value &amp; "*"</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
  </DataSets>
